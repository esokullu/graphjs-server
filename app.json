{
    "name": "GraphJS-Server",
    "description": "Serving GraphJS",
    "repository": "https://github.com/phonetworks/graphjs-server",
    "addons": ["rediscloud", "graphenedb", "cloudcube", "mailgun", "stream"],
    "env": {
          "PASSWORD_REMINDER": {
              "value": "/tmp/pins/"
          },
          "FOUNDER_EMAIL": {
              "value": "%s"
          },
          "FOUNDER_NICKNAME": {
              "value": "admin"
          },
          "FOUNDER_PASSWORD": {
              "generator": "secret"
          }
   }
}
